Vagrant.configure("2") do |config|
  config.vm.provision :shell, inline: "echo A"

  config.vm.define :testing do |test|
    	test.vm.box = "ubuntu/lunar64"
	test.vm.provision :shell, inline: "echo server"
  	test.vm.hostname = "server"
  end



  config.vm.define :testing do |pc|
        pc.vm.box = "ubuntu/lunar64"
        pc.vm.provision :shell, inline: "echo pc"
        pc.vm.hostname = "pc"
  end


  config.vm.provision :shell, inline: "echo C"
end
